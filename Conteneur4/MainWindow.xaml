<Window x:Class="Conteneur4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Conteneur4"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    
    <DockPanel>
        <Button DockPanel.Dock="Top">Top Button</Button>
        <Button DockPanel.Dock="Bottom">Top Bottom</Button>
        <Button DockPanel.Dock="Left">Top Left</Button>
        <Button DockPanel.Dock="Right">Top Right</Button>
        <Button>Top center</Button>
    </DockPanel>
    
</Window>
